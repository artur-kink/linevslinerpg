<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:state_pressed="true">
    <shape>
      <solid
          android:color="#cd6600" />
      <stroke
          android:width="3dp"
          android:color="#cd6600" />
      <corners android:radius="3dip"/>
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#964B00"
          android:endColor="#A75C00"
          android:angle="270" />
      <stroke
          android:width="3dp"
          android:color="#964B00" />
      <corners android:radius="3dip"/>
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>
